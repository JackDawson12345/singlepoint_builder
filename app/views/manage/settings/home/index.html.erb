<div>
  <h1 class="nata-sans page-title mb-5">Settings</h1>

  <div class="mb-5">
    <h2 class="nata-sans text-lg font-normal mb-2">Finance & Payments</h2>
    <div class="bg-gray-50 rounded-lg">
      <%= link_to manage_settings_payments_accept_payments_path, class: 'flex border-b py-4 px-6 hover:bg-blue-50' do %>
        <div class="w-4/5">
          <h3 class="nata-sans text-base font-semibold">Accept payments</h3>
          <p class="nata-sans text-sm font-normal">Choose the way you get paid by customers.</p>
        </div>
        <div class="w-1/5 flex justify-end items-center">
          <svg style="transform: rotate(270deg)" class="w-6 h-6 ml-1 transform transition-transform duration-200 text-blue-500" id="quick-actions-arrow" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
          </svg>
        </div>
      <% end %>
      <div class="flex border-b py-4 px-6 hover:bg-blue-50">
        <div class="w-4/5">
          <h3 class="nata-sans text-base font-semibold">Getting Paid Settings</h3>
          <p class="nata-sans text-sm font-normal">Manage your settings for invoices, price quotes, pay links, and related automations.</p>
        </div>
        <div class="w-1/5 flex justify-end items-center">
          <svg style="transform: rotate(270deg)" class="w-6 h-6 ml-1 transform transition-transform duration-200 text-blue-500" id="quick-actions-arrow" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
          </svg>
        </div>
      </div>
      <%= link_to manage_settings_payments_invoices_path, class: 'flex border-b py-4 px-6 hover:bg-blue-50' do %>
        <div class="w-4/5">
          <h3 class="nata-sans text-base font-semibold">Invoices</h3>
          <p class="nata-sans text-sm font-normal">Manage when invoices are sent and how they appear to customers.</p>
        </div>
        <div class="w-1/5 flex justify-end items-center">
          <svg style="transform: rotate(270deg)" class="w-6 h-6 ml-1 transform transition-transform duration-200 text-blue-500" id="quick-actions-arrow" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
          </svg>
        </div>
      <% end %>
    </div>
  </div>
  <div class="mb-5">
    <h2 class="nata-sans text-lg font-normal mb-2">Site, domain & SEO</h2>
    <div class="bg-gray-50 rounded-lg">
      <%= link_to manage_settings_website_seo_settings_path, class: 'flex border-b py-4 px-6 hover:bg-blue-50' do %>
        <div class="w-4/5">
          <h3 class="nata-sans text-base font-semibold">SEO Settings</h3>
          <p class="nata-sans text-sm font-normal">Customize your pages and meta tags for SEO and set your site preferences.</p>
        </div>
        <div class="w-1/5 flex justify-end items-center">
          <svg style="transform: rotate(270deg)" class="w-6 h-6 ml-1 transform transition-transform duration-200 text-blue-500" id="quick-actions-arrow" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
          </svg>
        </div>
      <% end %>
      <%= link_to manage_settings_website_website_settings_path, class: 'flex border-b py-4 px-6 hover:bg-blue-50' do %>
        <div class="w-4/5">
          <h3 class="nata-sans text-base font-semibold">Website settings</h3>
          <p class="nata-sans text-sm font-normal">Manage your site’s name, URL, favicon and more.</p>
        </div>
        <div class="w-1/5 flex justify-end items-center">
          <svg style="transform: rotate(270deg)" class="w-6 h-6 ml-1 transform transition-transform duration-200 text-blue-500" id="quick-actions-arrow" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
          </svg>
        </div>
      <% end %>
      <%= link_to manage_settings_website_domains_index_path, class: 'flex border-b py-4 px-6 hover:bg-blue-50' do %>
